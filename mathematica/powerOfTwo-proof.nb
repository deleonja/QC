Cell[TextData[{
 "Goal: find a proof that choosing the positive ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Tau]", 
    RowBox[{
     SubscriptBox["j", "1"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["j", "n"]}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9355b866-3d71-4a69-95f4-68fba900e8be"],
 "as the components left invariant and the negative ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Tau]", 
    RowBox[{
     SubscriptBox["j", "1"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["j", "n"]}]], TraditionalForm]],ExpressionUUID->
  "4979ba59-1f7e-4d19-a390-10a90dcf1097"],
 "as the components erased in every eigenvalue ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", 
    RowBox[{
     SubscriptBox["j", "1"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["j", "n"]}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cf521f6d-4b31-4a20-b8c5-1627edb128bc"],
 " the PCE operation is CP, i.e. every ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Lambda]", 
     RowBox[{
      SubscriptBox["j", "1"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["j", "n"]}]], "\[GreaterEqual]", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "63db1890-0484-4dc6-b75a-92bbb8b6d7c3"],
 "."
}], "Subsubsection",
